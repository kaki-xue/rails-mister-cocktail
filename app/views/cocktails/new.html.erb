<h1>Cocktails#new</h1>
<p>Find me in app/views/cocktails/new.html.erb</p>

<%= simple_form_for(@cocktail) do |f| %>
<%= f.input :name, label: "Name for your cocktail"%>
<%= f.input :description, label: "How's it special:"%>
<%= f.input :photo %>
<%= f.input :photo_cache, as: :hidden %>
<%= f.submit "Add in your bar" %>
<% end %>
